int main() {

	printf("Hello World\n");
	printf("%lf\n", sin(123.0));

	setlocale(LC_ALL, "en_US.UTF-8");

	{
		char buf[MB_LEN_MAX];
		int n = wctomb(buf, 0x65e5);
		int i;
		for(i = 0; i < n; ++i) {
			printf("%02x ", buf[i] & 0xff);
		}
		printf("\n");
	}

	{
		wchar_t wc;
		int r = mbtowc(&wc, "\xfe\xfe\xff\xff", 4);
		printf("%d: %08x\n", r, wc);
	}

	return 0;
}
